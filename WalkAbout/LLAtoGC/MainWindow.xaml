<Window x:Class="LLAtoGC.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LLAtoGC"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Label x:Name="label" Content="Enter Lat, Long, Alt " HorizontalAlignment="Left" Margin="87,26,0,0" VerticalAlignment="Top" Width="120">
            <Label.Background>
                <SolidColorBrush Color="{DynamicResource {x:Static SystemColors.InactiveCaptionColorKey}}"/>
            </Label.Background>
        </Label>
        <TextBlock x:Name="outputBlock1" HorizontalAlignment="Left" Margin="87,180,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Height="103" Width="325"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="102" Margin="87,57,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="311" TextChanged="textBox_TextChanged" AcceptsReturn="True" AcceptsTab="True"/>
        <Button x:Name="button" Content="Load from Clipboard" HorizontalAlignment="Left" Margin="267,31,0,0" VerticalAlignment="Top" Width="131" Height="21" Click="button_Click"/>

    </Grid>
</Window>
